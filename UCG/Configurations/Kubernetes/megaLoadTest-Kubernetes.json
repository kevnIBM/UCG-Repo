[{"test" : {"id" : "NodeRed-Scale-test",
            "phases" : ["convo-phase"],
            "plugins" : ["nodeRed-service"],
            "global_options" : [{"counter-new" : {"counter" : "Session-Id"}}]
 }},

 {"plugin" : {"id" : "nodeRed-service",
              "plugin_info" 
              : {"http-plugin" : {"servers" : [{"host" : "ucg-cluster.us-south.containers.mybluemix.net","port" : 80, "ssl" : false}
                                              ]
                                 }
                }
             }
 },

 {"phase" : {"id" : "convo-phase",
             "arrival_rate" : 60,
             "duration" : 600000,
             "concurrent_scenarios" : 50000,
             "rate" : 120,
             "scenarios" : [{"convo-scenario" : 1}]
           }
 },

 {"scenario" :
  {"id" : "convo-scenario", "keepalive" : true,
   "actions" : [ 
       
       {"counter-inc" : {"counter" : "Session-Id","increment" : 1}},
       
       {"http-request" : {"plugin_id" : "nodeRed-service",
                          "method" : "POST",
                          "path" : {"string-join" : {"strings" :  ["/load?user_id=kuber",{"counter-value" : {"counter" : "Session-Id"}},"&workspace_id=56cfca8a-b459-447f-ab35-e94511f6f7bc&text='Hello Insure Assist'&fname=jim"]}}                       
                         }
       },
       {"time-wait" : { "seconds" : 45 }},

       {"http-request" : {"plugin_id" : "nodeRed-service",
                          "method" : "POST",
                          "path" : {"string-join" : {"strings" :  ["/load?user_id=kuber",{"counter-value" : {"counter" : "Session-Id"}},"&workspace_id=56cfca8a-b459-447f-ab35-e94511f6f7bc&text='Status of my claim'&fname=jim"]}}                       
                         }
